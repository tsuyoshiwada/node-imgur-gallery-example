{
  "name": "node-imgur-gallery-example",
  "version": "1.0.0",
  "private": true,
  "description": "node-imgur-gallery-example",
  "scripts": {
    "start": "npm-run-all --parallel server watch",
    "server": "supervisor -w server server/index.js",
    "build": "npm-run-all clean build:dir --parallel build:assets build:js build:sass",
    "build:dir": "mkdirp public/css public/js",
    "build:assets": "ncp assets public",
    "build:js": "browserify -e src/js/app.js -o public/js/app.bundle.js -t [ babelify ]",
    "build:sass": "node-sass --output-style compressed -o public/css src/sass && postcss -u autoprefixer -r public/css/**/*.css",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "watchify -e src/js/app.js -o public/js/app.bundle.js -t [ babelify ] -v -d",
    "watch:sass": "watch 'npm run build:sass' src/sass/",
    "clean": "rimraf public",
    "uglify": "uglifyjs public/js/app.bundle.js -o public/js/app.bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src/js/**/*.js .eslintrc.js --no-ignore"
  },
  "dependencies": {
    "autobind-decorator": "^1.3.3",
    "babel-polyfill": "^6.13.0",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "imgur": "^0.1.7",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.0",
    "multer": "^1.2.0",
    "normalizr": "^2.2.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-stonecutter": "^0.3.3",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.11.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel": "^6.5.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "dotenv": "^2.0.0",
    "envify": "^3.4.1",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^3.9.3",
    "npm-run-all": "^3.1.0",
    "postcss-cli": "^2.6.0",
    "rimraf": "^2.5.4",
    "supervisor": "^0.11.0",
    "uglify-js": "^2.7.3",
    "watch": "^0.19.2",
    "watchify": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsuyoshiwada/node-imgur-gallery-example.git"
  },
  "author": "tsuyoshiwada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsuyoshiwada/node-imgur-gallery-example/issues"
  },
  "homepage": "https://github.com/tsuyoshiwada/node-imgur-gallery-example#readme"
}
